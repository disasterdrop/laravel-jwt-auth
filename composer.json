{
  "name": "disasterdrop/laravel-jwt-auth",
  "description": "JWT Authentication Server/Client for Laravel",
  "keywords": [
    "laravel",
    "musterhaus",
    "authentication",
    "auth",
    "jwt",
    "api"
  ],
  "authors": [
    {
      "name": "Sebastian HÃ¼bner",
      "email": "huebner@musterhaus.net",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "laravel/framework": ">=5.6",
    "firebase/php-jwt": "^5.0",
    "guzzlehttp/guzzle": "^6.3",
    "ramsey/uuid": "^3.7",
    "sonata-project/google-authenticator": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Musterhaus\\LaravelJWTAuth\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Musterhaus\\LaravelJWTAuth\\Providers\\AuthServiceProvider"
      ]
    }
  }
}
